<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.RegisterAllActivity"
    android:background="#FFF3F3F3">



    <RelativeLayout
        android:id="@+id/register_all_tou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/dp_35"
        android:background="#fff"
        android:paddingBottom="@dimen/dp_14"
        >
        <TextView
            android:id="@+id/register_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="＜"
            android:textSize="@dimen/dp_18"
            android:layout_marginLeft="@dimen/dp_12"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:textSize="@dimen/dp_18"
            android:text="基本信息完善"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/register_all_RelativeLayout_tou"
        android:layout_below="@+id/register_all_tou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_register_all_back"
        android:layout_marginLeft="@dimen/dp_14"
        android:layout_marginRight="@dimen/dp_14"
        android:layout_marginBottom="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_15"
            android:orientation="horizontal"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:background="@drawable/shape_register_edittext"
            android:paddingBottom="@dimen/dp_10">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="店铺名称"
            android:textSize="@dimen/dp_16"/>
        <EditText
            android:id="@+id/register_all_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:layout_marginLeft="@dimen/dp_75"
            android:layout_marginRight="@dimen/dp_20"
            android:hint="请输入店铺名称"
            android:textColor="#FFCCCCCC"
            android:background="@null"/>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/register_all_RelativeLayout_body"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/register_all_RelativeLayout_tou"
        android:layout_marginTop="1dp"
        android:background="@drawable/shape_register_all_back"
        android:layout_marginLeft="@dimen/dp_14"
        android:layout_marginRight="@dimen/dp_14"
        android:paddingBottom="@dimen/dp_10">

        <RelativeLayout
            android:id="@+id/register_all_RelativeLayout_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_15"
            android:orientation="horizontal"
            android:background="@drawable/shape_register_edittext"
            android:paddingBottom="@dimen/dp_18">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="店铺类别"
                android:textSize="@dimen/dp_16"
                />
            <TextView
                android:id="@+id/register_all1_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_marginRight="@dimen/dp_20"
                android:hint="店铺类别"
                android:textColor="#FFCCCCCC"
                android:textSize="@dimen/dp_16"
                android:background="@null"/>

            <TextView
                android:id="@+id/register_all1_type_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=">"
                android:layout_alignParentRight="true"
                android:paddingLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:textSize="@dimen/dp_16"
                android:textColor="#FF333333"
                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_below="@+id/register_all_RelativeLayout_one"
            android:id="@+id/register_all_RelativeLayout_theer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_18"
            android:orientation="horizontal"
            android:background="@drawable/shape_register_edittext"
            android:paddingBottom="@dimen/dp_18">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="外卖服务电话"
                android:textSize="@dimen/dp_16" />
            <EditText
                android:id="@+id/register_all_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_marginRight="@dimen/dp_20"
                android:hint="请输入联系人手机号"
                android:maxLength="11"
                android:inputType="phone"
                android:textColor="#FFCCCCCC"
                android:background="@null"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/register_all_RelativeLayout_theer"
            android:id="@+id/register_all_RelativeLayout_four"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_18"
            android:orientation="horizontal"
            android:background="@drawable/shape_register_edittext"
            android:paddingBottom="@dimen/dp_18">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="店铺分类"
                android:textSize="@dimen/dp_16"/>
            <TextView
                android:id="@+id/register_all_class"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_marginRight="@dimen/dp_20"
                android:hint="中餐"
                android:textColor="#FFCCCCCC"
                android:textSize="@dimen/dp_16"
                android:background="@null"/>
            <TextView
                android:id="@+id/register_all_class_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=">"
                android:layout_alignParentRight="true"
                android:paddingLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:textSize="@dimen/dp_16"
                android:textColor="#FF333333"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/register_all_RelativeLayout_four"
            android:id="@+id/register_all_RelativeLayout_five"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_18"
            android:orientation="horizontal"
            android:background="@drawable/shape_register_edittext"
            android:paddingBottom="@dimen/dp_18">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="所属区域"
                android:textSize="@dimen/dp_16"/>
            <TextView
                android:id="@+id/register_all_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_110"
                android:hint="郑州 - 二七"
                android:textSize="@dimen/dp_16"
                android:textColor="#FFCCCCCC"
                android:background="@null"/>
            <TextView
                android:id="@+id/register_all_home_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=">"
                android:layout_alignParentRight="true"
                android:paddingLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_15"
                android:textSize="@dimen/dp_16"
                android:textColor="#FF333333"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@+id/register_all_RelativeLayout_five"
            android:id="@+id/register_all_RelativeLayout_six"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_18"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/dp_15">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="详细地址"
                android:textSize="@dimen/dp_16"/>
            <EditText
                android:id="@+id/register_all_detailed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="18"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_marginRight="@dimen/dp_20"
                android:hint="请填写店铺详细地址"
                android:textColor="#FFCCCCCC"
                android:background="@null"/>
        </RelativeLayout>

    </RelativeLayout>
    <Button
        android:id="@+id/register_all_finish"
        android:layout_width="@dimen/dp_250"
        android:layout_height="@dimen/dp_40"
        android:layout_below="@+id/register_all_RelativeLayout_body"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="@dimen/dp_60"
        android:background="@drawable/shape_mainactivity_button"
        android:text="提交"
        android:textColor="#fff" />
</RelativeLayout>
